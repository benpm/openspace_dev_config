{
    "version": 4,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 19,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "windows-min",
            "description": "no tests, no extraneous modules, no vcpkg, no warnings",
            "generator": "Visual Studio 17 2022",
            "binaryDir": "${sourceDir}/build",
            "warnings": {
                "dev": false,
                "deprecated": false,
                "systemVars": false
            },
            "environment": {
                "VCPKG_ROOT": "",
                "VCPKG_DEFAULT_TRIPLET": ""
            },
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "GHOUL_HAVE_TESTS": "OFF",
                "OPENSPACE_HAVE_TESTS": "OFF",
                "OPENSPACE_MODULE_VIDEO": "OFF",
                "OPENSPACE_MODULE_TOYVOLUME": "OFF",
                "SGCT_BUILD_TESTS": "OFF",
                "ASSIMP_INSTALL_PDB": "OFF",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
                "CMAKE_TOOLCHAIN_FILE": "",
                "DISABLE_VCPKG": "ON",
                "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "support/cmake/disable_vcpkg.cmake"
            },
            "hidden": false
        }
    ],
    "buildPresets": [
        {
            "name": "windows-min",
            "configuration": "RelWithDebInfo",
            "configurePreset": "windows-min",
            "jobs": 6,
            "targets": [
                "OpenSpace"
            ],
            "inheritConfigureEnvironment": true,
            "hidden": false
        }
    ]
}